{
  "expo": {
    "name": "Send Direct Message",
    "slug": "send-direct-message",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#128C7E"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.example.senddirectmessage",
      "versionCode": 6,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#128C7E"
      },
      "config": {
        "googleMobileAdsAppId": "google-ads-code"
      },
      "permissions": [
        "FOREGROUND_SERVICE"
      ]
    }
  }
}
